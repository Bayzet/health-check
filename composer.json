{
    "name": "bayzet/health-check",
    "description": "Health check bundle",
    "type": "symfony-bundle",
    "license": "proprietary",
    "require": {
        "php": "7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/flex": "*",
        "symfony/framework-bundle": "*",
        "sensio/framework-extra-bundle": "*",
        "symfony/lts": "*",
        "symfony/yaml": "*"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "bayzet\\HealthCheckBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bayzet\\HealthCheckBundle\\Tests\\": "tests"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
